{
  "name": "genome-designer",
  "version": "0.0.0",
  "description": "Genome Designer",
  "main": "index.js",
  "scripts": {
    "start": "babel-node tools/run start",
    "start-instance": "babel-node tools/run start-instance",
    "clean": "babel-node tools/run clean",
    "build": "babel-node tools/run build",
    "build-client": "babel-node tools/run bundle",
    "postinstall": "npm run install-extensions && npm run selenium",
    "install-extensions": "cd server/extensions && rm -rf node_modules && npm install --only=prod --no-bin-link --no-optional",
    "check": "npm run lint && npm run test",
    "lint": "eslint src server plugins; exit 0;",
    "lint-jscs": "jscs src server",
    "mongo": "mongod --dbpath ./mongo-local",
    "test": "rm -rf ./storage/test && NODE_ENV=test mocha --recursive --compilers js:babel-register,css:test/css-null-compiler.js --require ./test/setup.js --timeout=25000",
    "test:watch": "npm test -- --watch",
    "nightwatch": "node ./node_modules/nightwatch/bin/nightwatch --config ./test-e2e/nightwatch.json",
    "e2e": "node ./bin/e2e.js",
    "screenshots": "rm -f ./tests/screenshots/",
    "selenium": "node ./node_modules/selenium-standalone/bin/selenium-standalone install",
    "start-docker": "docker build -t genome-designer . && docker run --privileged -v /var/run/docker.sock:/run/docker.sock -i -p 3000:3000 -t genome-designer",
    "create-ecoli-db": "python extensions/foundry/ecoli/generate_db.py jdon extensions/foundry/ecoli/ecoli.genes.json extensions/foundry/ecoli.json",
    "auth": "./bin/local-auth.sh",
    "auth-test": "COMMAND=\"mocha --compilers js:babel-register,css:test/css-null-compiler.js --require ./test/setup.js --timeout=10000 test/server/auth/REST.spec.js\" ./bin/server-auth.sh",
    "server-auth": "EMAIL=1 COMMAND='npm run start-instance' ./bin/server-auth.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autodesk-bionano/genome-designer.git"
  },
  "author": "",
  "license": "MIT",
